							%input{:accept => "image/*", :name => "file", "ng-model" => "image", "ngf-max-size" => "2MB", "ngf-model-invalid" => "errorFile", "ngf-select" => "", :required => "", :type => "file"}/
						.file-path-wrapper
							%input.file-path.validate{:type => "text"}/
	.modal-footer
		%a.modal-action.modal-close.waves-effect.waves-green.btn-flat{:"ng-click"=>"noticia_edit(image)", :"ng-disabled" => "!myForm.$valid"} Guardar
#nav.modal
	.modal-content
		.row
			.col.s12
				.row
					.input-field.col.s12
						%label{:for => "name"}Nombre
						%input.validate#name{:type => "text",:placeholder=>"Name",:"ng-model"=>"nav.name"}/
